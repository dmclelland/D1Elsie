        </class>
        <class name="com.dmc.d1.test.event.StringUpdatedEvent2">
            <field name="str" type="java.lang.String"/>
        </class>
        <class name="com.dmc.d1.test.event.TriggerExceptionEvent">
        </class>
        <class name="com.dmc.d1.test.event.TriggerExceptionNestedEvent">
        </class>
        <class name="com.dmc.d1.test.event.TriggerExceptionInNestedAggregateEvent">
            <field name="nestedId" type="java.lang.String"/>
